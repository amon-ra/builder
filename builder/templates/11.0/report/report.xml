<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="report_sale_commision_template">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="web.external_layout">
                        <div class="page">
                            <h2>Report title</h2>
                            <p>This object's name is <span t-field="o.name"/></p>
                            <span t-field="o.commission_total"/>
                        </div>
                    </t>
                </t>
            </t>
        </template>    
        <report
            id="report_sale_commision_invoice"
            model="account.invoice"
            string="Commisions"
            report_type="qweb-pdf"
            name="sale_commision_report.report_sale_commision_template"
            file="sale_commision_report.report_sale_commision_template"
            attachment_use="False"
            attachment="(object.user_id.name+'.pdf')"
        />

    </data>
</odoo>